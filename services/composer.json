{
    "name": "islandora/services",
    "description": "RESTful services container",
    "repositories": [
      {
                "type": "path",
                "url": "ResourceService"
      },
      {
                "type": "path",
                "url": "CollectionService"
      },
      {
                "type": "path",
                "url": "TransactionService"
      }
    ],
    "require": {
        "islandora/chullo": "^0.0",
        "islandora/resource-service": "dev-sprint-002",
        "islandora/collection-service": "dev-sprint-002",
        "silex/silex": "^1.3",
        "symfony/config": "^3.0",
        "twig/twig": "^1.23",
        "symfony/yaml": "^3.0",
        "ramsey/uuid": "^3.1"
    },
    "autoload": {
        "psr-4": {"Islandora\\Services\\": "src/"}
    },
    "minimum-stability" : "dev",
    "authors": [
        {
            "name": "Jared Whiklo",
            "email": "jwhiklo@gmail.com"
        }
    ]
